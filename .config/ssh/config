Host github.com
    ForwardAgent yes
    Hostname ssh.github.com
    Port 443
    User git

Host *.onion
    ProxyCommand connect -S localhost:9050 %h %p

Host *
    StrictHostKeyChecking no
    UserKnownHostsFile ~/.config/ssh/known_hosts
    ConnectTimeout 10

Include ~/persist/ssh/config
Include ~/work/ssh/config
