#!/usr/bin/env zsh

dirs=(.nv .renpy .swt .java .pkg-cache .vcpkg .stellarium .mozilla .bitmonero .npm .pgadmin .android .eclipse .m2 .dotnet .nyx .gitkraken go .yarn .tabby-client .nix-defexpr)
files=(.yarnrc .wget-hsts .python_history .nvidia-settings-rc .mysql_history .gitconfig .pulse-cookie .nix-profile)

for dir in $dirs; do
    rm ~/$dir -r &>/dev/null && echo "removed '$dir'"
done

for file in $files; do
    rm ~/$file &>/dev/null && echo "removed '$file'"
done
