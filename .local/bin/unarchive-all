#!/usr/bin/env bash

find -type f -name "*.zip" -exec unzip {} \;
find -type f -name "*.tar.gz" -exec tar -xvzf {} \;
find -type f -name "*.tar.bz2" -exec tar -xvjf {} \;
find -type f -name "*.tar.xz" -exec tar -xvJf {} \;
find -type f -name "*.rar" -exec unrar x {} \;
find -type f -name "*.7z" -exec 7z x {} \;
